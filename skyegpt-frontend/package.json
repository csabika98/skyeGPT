{
  "name": "skyegpt-frontend",
  "version": "0.3.8",
  "private": true,
  "type": "module",
  "scripts": {
    "preset": "node prebuild.mjs",
    "preset:dev": "node prebuild.mjs --dev",
    "preset:set-env": "node prebuild.mjs --setenv-only",
    "dev": "npm run preset:dev && next dev -p 5173 --turbopack",
    "build": "next build",
    "start": "next start -p 5173",
    "lint": "next lint",
    "test": "vitest run",
    "prebuild": "npm run preset"
  },
  "dependencies": {
    "dotenv": "^16.5.0",
    "lodash": "^4.17.21",
    "next": "15.3.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-markdown": "^10.1.0",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/lodash": "^4.17.16",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-react": "^4.4.1",
    "eslint": "^9",
    "eslint-config-next": "15.3.1",
    "remark-gfm": "^4.0.1",
    "tailwindcss": "^4",
    "typescript": "^5",
    "vitest": "^3.1.2"
  }
}